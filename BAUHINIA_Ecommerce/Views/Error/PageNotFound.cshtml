@{
    ViewBag.Title = "404";
    Layout = null;
}


<!DOCTYPE html>
<html lang="en"
      class="light-style customizer-hide"
      dir="ltr"
      data-theme="theme-default"
      data-template="vertical-menu-template-free">

<head>
    <meta charset="utf-8" />
    <meta name="viewport"
          content="width=device-width, initial-scale=1.0, user-scalable=no, minimum-scale=1.0, maximum-scale=1.0" />
    <title>@ViewData["Title"] | BAUHINIA </title>

    <meta name="description" content="" />

    <link rel="icon" type="image/x-icon" href="~/assets/be/img/favicon/faviconb.ico" />

    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link href="https://fonts.googleapis.com/css2?family=Public+Sans:ital,wght@0,300;0,400;0,500;0,600;0,700;1,300;1,400;1,500;1,600;1,700&display=swap"
          rel="stylesheet" />

    <link rel="stylesheet" href="~/assets/be/vendor/fonts/boxicons.css" />

    <link rel="stylesheet" href="~/assets/be/vendor/css/core.css" class="template-customizer-core-css" />
    <link rel="stylesheet" href="~/assets/be/vendor/css/theme-default.css" class="template-customizer-theme-css" />
    <link rel="stylesheet" href="~/assets/be/css/demo.css" />

    <link rel="stylesheet" href="~/assets/be/vendor/libs/perfect-scrollbar/perfect-scrollbar.css" />

    <!-- Page -->
    <link rel="stylesheet" href="~/assets/be/vendor/css/pages/page-misc.css" />

    <link rel="stylesheet" href="~/assets/be/vendor/css/pages/page-auth.css" />
    <script src="~/assets/be/vendor/js/helpers.js"></script>

    <script src="~/assets/be/js/config.js"></script>


</head>

<body>
    <!-- Error -->
    <div class="container-xxl container-p-y">
        <div class="misc-wrapper">
            <h2 class="mb-2 mx-2">Page Not Found :(</h2>
            <p class="mb-4 mx-2">Oops! 😖 The requested URL was not found on this server.</p>
            <a asp-action="Index" asp-controller="Dashboard" class="btn btn-primary">Back to home</a>
            <div class="mt-3">
                <img src="~/assets/be/img/illustrations/page-misc-error-light.png"
                     alt="page-misc-error-light"
                     width="500"
                     class="img-fluid"
                     data-app-dark-img="illustrations/page-misc-error-dark.png"
                     data-app-light-img="illustrations/page-misc-error-light.png" />
            </div>
        </div>
    </div>
    <!-- /Error -->
    <!-- / Content -->

     <!-- Core JS -->
    <!-- build:js assets/vendor/js/core.js -->
    <script src="~/assets/be/vendor/libs/jquery/jquery.js"></script>
    <script src="~/assets/be/vendor/libs/popper/popper.js"></script>
    <script src="~/assets/be/vendor/js/bootstrap.js"></script>
    <script src="~/assets/be/vendor/libs/perfect-scrollbar/perfect-scrollbar.js"></script>

    <script src="~/assets/be/vendor/js/menu.js"></script>
    <!-- endbuild -->

    <!-- Vendors JS -->

    <!-- Main JS -->
    <script src="~/assets/be/js/main.js"></script>

    <script async defer src="https://buttons.github.io/buttons.js"></script>
</body>

</html>